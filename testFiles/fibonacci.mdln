'calculate nth fibonacci number
function fib(n) {
	if(n=0) {
		return n
	} else if(n=1) {
		return n
	} else {
		return call fib(n-1) + call fib(n-2)
	}
}

main {

    call display("Enter the number for calculating fibonacci: ")
    call newline()
    var c = call readInteger()
    call display("Fibonacci("+c+") = "+call fib(c))
    
}
